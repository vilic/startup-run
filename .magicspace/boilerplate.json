{
  "$schema": "boilerplate.schema.json",
  "boilerplate": "@mufan/code-boilerplates/typescript",
  "options": {
    "name": "startup-run",
    "description": "Run scripts at startup with ease.",
    "repository": "https://github.com/vilic/startup-run.git",
    "license": "MIT",
    "author": "vilicvane",
    "badges": {
      "npm": true,
      "repo": true,
      "license": true
    },
    "type": "module",
    "defaultBranch": "master",
    "packageManager": "pnpm",
    "projects": [
      {
        "name": "library"
      },
      {
        "name": "daemon",
        "references": ["library"]
      },
      {
        "name": "test",
        "references": ["library"]
      }
    ]
  }
}
